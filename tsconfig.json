{
  "compilerOptions": {
    "target": "es5",                  
    "lib": ["es5", "dom"],           
    "types": ["cypress", "node"],    
    "module": "commonjs",            
    "strict": true,                 
    "esModuleInterop": true,        
    "forceConsistentCasingInFileNames": true, 
    "skipLibCheck": true             
  },
  "include": ["**/*.ts"],          
  "exclude": ["node_modules"]        
}